<h1>Hello, User!</h1>
<h2>Please enter a new city: </h2>
<%= form_with local: true, url: 'create', method: "post" do |form| %>
    <p>City: <%= form.text_field :city %></p> <br />
    <p>Population: <%= form.text_field :population %></p> <br />
    <p>Landmark: <%= form.text_field :landmark %></p> <br />
    <%= form.submit %>
<% end %>
